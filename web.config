<?meta http-equiv="Content-Type" content="text/html; charset=utf-8" /?> 
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="Default.php" />
                <add value="index.php" />
            </files>
        </defaultDocument>
        <handlers>
            <add name="php" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="D:\php\php-cgi.exe" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
